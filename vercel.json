{
  "version": 2,
  "builds": [
    {
      "src": "social_media_feed/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build/static"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "social_media_feed/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "$PYTHONPATH:/var/task"
  },
  "functions": {
    "social_media_feed/wsgi.py": {
      "maxDuration": 30
    }
  }
}
