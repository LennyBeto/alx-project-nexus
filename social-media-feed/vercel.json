{
  "buildCommand": "cd social-media-feed && npm install && npm run build",
  "outputDirectory": "social-media-feed/build",
  "devCommand": "cd social-media-feed && npm start",
  "installCommand": "cd social-media-feed && npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
